   "source": [
    "n = int(input(\"Digite o valor de N: \"))\n",
    "\n",
    "s = 0\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    s += i / (n+1 - i)\n",
    "    print(\"A soma de {:.2f} / {:.2f} é = {:.2f}. Soma acumulada = {:.2f}\".format(i,(n+1 - i), i / (n+1 - i), s))"
   ]
